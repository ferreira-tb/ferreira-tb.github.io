{
  "name": "@tb-dev/me",
  "version": "0.8.13",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run fetch && pnpm run format && miho bump -k -a . -m \"chore: deploy\"",
    "dev": "vite",
    "fetch": "pwsh scripts/fetch.ps1",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.0.8",
    "@tb-dev/vue": "^2.0.14",
    "@tb-dev/vue-components": "^5.0.2",
    "@vueuse/core": "^13.5.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tb-dev/eslint-config": "^8.0.3",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.3"
  }
}
