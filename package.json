{
  "name": "@tb-dev/me",
  "version": "0.8.8",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run fetch && pnpm run format && miho bump -k -a . -m \"chore: deploy\"",
    "dev": "vite",
    "fetch": "pwsh scripts/fetch.ps1",
    "format": "prettier . --write",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.0.0",
    "@tb-dev/vue": "^2.0.4",
    "@tb-dev/vue-components": "^0.2.1",
    "@vueuse/core": "^13.2.0",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "@tb-dev/eslint-config": "^7.1.4",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "tailwindcss": "^4.1.7",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}