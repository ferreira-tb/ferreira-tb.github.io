{
  "name": "website",
  "version": "0.8.25",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run fetch && pnpm run format && miho bump -k -a . -m \"chore: deploy\"",
    "dev": "vite",
    "fetch": "pwsh scripts/fetch.ps1",
    "format": "dprint fmt",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.0.19",
    "@tb-dev/vue": "^3.4.1",
    "@tb-dev/vue-components": "^7.0.1",
    "@vueuse/core": "^14.1.0",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tb-dev/eslint-config": "^8.2.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "eslint": "^9.39.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.5"
  }
}