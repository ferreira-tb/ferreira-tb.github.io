{
  "name": "website",
  "version": "0.8.22",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run fetch && pnpm run format && miho bump -k -a . -m \"chore: deploy\"",
    "dev": "vite",
    "fetch": "pwsh scripts/fetch.ps1",
    "format": "dprint fmt",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.0.18",
    "@tb-dev/vue": "^3.3.4",
    "@tb-dev/vue-components": "^7.0.0",
    "@vueuse/core": "^14.0.0",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@tb-dev/eslint-config": "^8.1.1",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.39.0",
    "tailwindcss": "^4.1.16",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vue-tsc": "^3.1.3"
  }
}