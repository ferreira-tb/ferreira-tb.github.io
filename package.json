{
  "name": "website",
  "version": "0.8.30",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "build": "vite build",
    "deploy": "pnpm run fetch && pnpm run format && miho bump -k -a . -m \"chore: deploy\"",
    "dev": "vite",
    "fetch": "pwsh scripts/fetch.ps1",
    "format": "dprint fmt",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update major -t"
  },
  "dependencies": {
    "@tb-dev/utils": "^7.3.1",
    "@tb-dev/vue": "^4.0.2",
    "@tb-dev/vue-components": "^7.0.9",
    "@vueuse/core": "^14.2.0",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tb-dev/eslint-config": "^8.2.6",
    "@types/node": "^25.2.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "eslint": "^9.39.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
}