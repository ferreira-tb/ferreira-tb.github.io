{
  "name": "@tb-dev/me",
  "version": "0.1.1",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write",
    "*.{?(c|m)@(j|t)s,vue}": "pnpm lint"
  },
  "scripts": {
    "build": "pnpm run type-check && vite build",
    "bump": "miho bump --no-ask --add .",
    "dev": "vite",
    "prepare": "husky install",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "preview": "vite preview",
    "repo": "cargo run --manifest-path=cli/Cargo.toml -- repo",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "manatsu": "^0.3.5",
    "vue": "~3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^1.6.2",
    "@tb-dev/utility-types": "^1.0.10",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "~5.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-vue-devtools": "7.0.11",
    "vue-tsc": "^1.8.27"
  }
}
