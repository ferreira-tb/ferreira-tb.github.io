{
  "vsicons.associations.folders": [
    { "icon": "theme", "extensions": ["composables"], "format": "svg" },
    {
      "icon": "component",
      "extensions": ["components"],
      "format": "svg",
      "overrides": "component"
    }
  ],
  "editor.tabSize": 2,
  "files.exclude": {
    ".eslintcache": true
  },
  "prettier.requireConfig": true,
  "editor.defaultFormatter": null,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "isort.args": ["--profile", "black"],
  "tailwindCSS.classAttributes": [
    "body-class",
    "bottom-class",
    "center-class",
    "content-class",
    "dialog-class",
    "end-class",
    "footer-class",
    "header-class",
    "header-end-class",
    "header-start-class",
    "input-class",
    "logo-class",
    "media-class",
    "menu-class",
    "menu-item-class",
    "sidebar-class",
    "sidebar-item-class",
    "start-class",
    "subtitle-class",
    "tbody-class",
    "tbody-row-class",
    "thead-class",
    "thead-row-class",
    "title-class",
    "title-link-class",
    "top-class"
  ]
}
